{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "module": "CommonJS",
    "lib": [
      "es2015",
      "dom"
    ],
    "target": "es5",
    "allowJs": true,
    "sourceMap": true,
    "declaration": true,
    "outDir": "dist"
  },
  "files": [
    "src/index.ts"
  ],
  "paths": {
    "ckeditor/*": [
      "./node_modules/@ckeditor/ckeditor5-core/*"
    ],
    "@mudde-core":[
      "./node_modules/mudde-core/src"
    ],
  }
}