{
   "compilerOptions": {
      "module": "AMD",
      "target": "ES2015",
      "lib": [
         "ES2015",
         "DOM"
      ],
      "moduleResolution": "Node",
      "strict": true,
      "noImplicitAny": false,
      "removeComments": true,
      "skipLibCheck": true,
      "inlineSourceMap": true,
      "baseUrl": "./src/Mudde",
      "outFile": "./dist/formjs.js",
      "paths": {
         "mudde-node/src/Mudde/Core/Node": [
            "../../node_modules/mudde-node/src/node"
         ],
         "Mudde/*": [
            "./*"
         ]
      }
   },
   "include": [
      "src/**/*"
   ],
   "exclude": [
      "dist",
      "node_modules"
   ]
}