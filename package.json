{
  "name": "@mudde/formgen",
  "version": "1.0.0",
  "description": "Create a HTML form with a JSON as input",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --reporter spec --require node_modules/ts-node/register --require node_modules/amd-loader/ test/**/*.ts",
    "build": "npx webpack --mode=production --node-env=production && cp dist/formgen.* ../formgen-demo-with-symfony/public/example/script/",
    "build:dev": "npx webpack --mode=development && cp dist/formgen.* ../formgen-demo-with-symfony/public/example/script/",
    "build:prod": "npx webpack --mode=production --node-env=production && cp dist/formgen.* ../formgen-demo-with-symfony/public/example/script/",
    "watch": "npx webpack --watch",
    "serve": "npx webpack serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mudde/formgen.git"
  },
  "keywords": [
    "form",
    "generator"
  ],
  "author": "olaf mudde <olaf.mudde@xs4all.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mudde/formgen/issues"
  },
  "homepage": "https://github.com/mudde/formgen#readme",
  "workspaces": {
    "packages": [
      "ckeditor5-build-classic-master"
    ]
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^27.1.0",
    "@ckeditor/ckeditor5-typing": "^27.1.0",
    "bootstrap": "^5.0.0-beta3",
    "ckeditor5": "^27.1.0",
    "i": "^0.3.6",
    "jquery": "^3.6.0",
    "jquery-validation": "^1.19.3",
    "jsdom": "^16.5.3",
    "mudde-core": "github:mudde/core",
    "select2": "*",
    "semantic-ui-flag": "^2.4.0",
    "tinycrop": "^1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@types/chai": "^4.2.17",
    "@types/jquery": "^3.5.6",
    "@types/mocha": "^8.2.2",
    "@webpack-cli/generators": "^2.2.0",
    "amd-loader": "^0.0.8",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "create-ts-index": "^1.13.6",
    "ctix": "^0.3.1",
    "mocha": "^8.3.2",
    "npm": "^7.11.1",
    "prettier": "^2.3.2",
    "requirejs": "^2.3.6",
    "ts-loader": "^9.2.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
